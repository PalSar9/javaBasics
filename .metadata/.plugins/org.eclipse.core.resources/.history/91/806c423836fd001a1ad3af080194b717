
public class runningSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] nums= {1,2,3,4,5};
		rs(nums);
	}
	static void rs(int[] nums) {
		int[] sum=new int[nums.length];
		for(int i=0;i<nums.length;i++)
		{
			for(int j=i;j>=0;i--) {
				sum[i]+=nums[j];
			}
		}
		for(int i=0;i<nums.length;i++)
			System.out.println(sum[i]);
	}
}

class threadSleep extends Thread
{
	public void run() {
	for(int i=0;i<5;i++)
	{
		try {
			Thread.sleep(4000);
		}
		catch(Exception e) {
			
		}
		System.out.println(i);
	}
}}
public class ThrowExample
{
public static void main(String[] args) {
	threadSleep obj=new threadSleep();
	obj.start();
}}
	
